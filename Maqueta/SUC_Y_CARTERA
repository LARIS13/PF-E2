CUMPLIMIENTO_METAS_VARIACION AS VARIACION,
  CASE
    WHEN CUMPLIMIENTO_METAS >= 0.98
      THEN '游릭 Optimo'
    WHEN CUMPLIMIENTO_METAS > 0.85
      THEN '游리 En observaci칩n'
    WHEN CUMPLIMIENTO_METAS <= 0.85
      THEN '游댮 Cr칤tico'
  END AS Estado,
 
  CASE
    WHEN CUMPLIMIENTO_METAS > LAG(CUMPLIMIENTO_METAS) OVER (PARTITION BY Sucursal,EXTRACT(MONTH FROM PERIODO) ORDER BY PERIODO) AND CUMPLIMIENTO_METAS > CUMPLIMIENTO_METAS_Zona
      THEN 'Mejor que la zona y con tendencia en alza'
 
    WHEN CUMPLIMIENTO_METAS < LAG(CUMPLIMIENTO_METAS) OVER (PARTITION BY Sucursal,EXTRACT(MONTH FROM PERIODO) ORDER BY PERIODO) AND CUMPLIMIENTO_METAS > CUMPLIMIENTO_METAS_Zona
      THEN 'Mejor que la zona y con tendencia a la baja'
 
    WHEN CUMPLIMIENTO_METAS > LAG(CUMPLIMIENTO_METAS) OVER (PARTITION BY Sucursal,EXTRACT(MONTH FROM PERIODO) ORDER BY PERIODO) AND CUMPLIMIENTO_METAS < CUMPLIMIENTO_METAS_Zona
      THEN 'Peor que la zona y Con tendencia en alza'
 
    WHEN CUMPLIMIENTO_METAS < LAG(CUMPLIMIENTO_METAS) OVER (PARTITION BY Sucursal,EXTRACT(MONTH FROM PERIODO) ORDER BY PERIODO) AND CUMPLIMIENTO_METAS < CUMPLIMIENTO_METAS_Zona
      THEN 'Peor que la zona y Con tendencia a la baja'
  END AS Dictamen,
 
CASE
    WHEN CUMPLIMIENTO_METAS >= 0.8
      THEN ' Felicitaci칩n p칰blica y personal al equipo y responsables, Pedir retroalimentaci칩n al equipo durante las minijuntas sobre qu칠 funcion칩 bien'
    WHEN CUMPLIMIENTO_METAS > 0.7
      THEN 'Mantener rutinas de seguimiento efectivo (revisi칩n de metas diarias o semanales),  Motivar al equipo a identificar oportunidades de mejora o eficiencia propia'
    WHEN CUMPLIMIENTO_METAS <= 0.7
      THEN 'Establecer Reuniones diarias breves de enfoque con colaboradores resagados para recuperar el cumplimiento, Activar micro-capacitaciones semanales con ejemplos pr치cticos'
  END AS RECOMENDACION
 
FROM `BI_SUCURSALES_BANCO.ZZ_MOCK_SUC_Y`
 
